{
  "partitions": [
    {
      "defaults": {
        "hostname": "{service}.{region}.{dnsSuffix}",
        "protocols": ["http", "https"],
        "signatureVersions": ["v4"]
      },
      "dnsSuffix": "k2.cloud",
      "partition": "k2",
      "partitionName": "K2 Cloud Standard",
      "regionRegex": "^ru-msk|ru-spb$",
      "regions": {
        "ru-msk": {
          "description": "Moscow"
        },
        "ru-spb": {
          "description": "Saint Petersburg"
        }
      },
      "services": {
        "autoscaling": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "backup": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "cloudtrail": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "cloudwatch": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "directconnect": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "ec2": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "efs": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "eks": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "eks-legacy": {
          "endpoints": {
            "ru-msk": {
              "hostname": "eks.ru-msk.k2.cloud/legacy"
            }
          }
        },
        "elb": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "iam": {
          "endpoints": {
            "k2-global": {
              "credentialScope": {
                "region": "ru-msk"
              },
              "hostname": "iam.k2.cloud"
            }
          },
          "isRegionalized": false,
          "partitionEndpoint": "k2-global"
        },
        "paas": {
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {}
          }
        },
        "route53": {
          "endpoints": {
            "k2-global": {
              "credentialScope": {
                "region": "ru-msk"
              },
              "hostname": "route53.k2.cloud"
            }
          },
          "isRegionalized": false,
          "partitionEndpoint": "k2-global"
        },
        "s3": {
          "defaults": {
            "signatureVersions": ["s3v4"]
          },
          "endpoints": {
            "ru-msk": {},
            "ru-spb": {},
            "k2-global": {
              "credentialScope": {
                "region": "ru-msk"
              },
              "hostname": "s3.k2.cloud",
              "signatureVersions": ["s3", "s3v4"]
            }
          },
          "isRegionalized": true,
          "partitionEndpoint": "k2-global"
        }
      }
    }
  ],
  "version": 3
}
